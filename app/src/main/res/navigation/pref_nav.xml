<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/pref_nav"
    app:startDestination="@id/preferenceFragment">
    <fragment
        android:id="@+id/preferenceFragment"
        android:name="com.osfans.trime.ui.fragments.PreferenceFragment"
        android:label="@string/app_name" >
        <action
            android:id="@+id/action_preferenceFragment_to_licenseFragment"
            app:destination="@id/licenseFragment" />
    </fragment>
    <fragment
        android:id="@+id/licenseFragment"
        android:name="com.osfans.trime.ui.fragments.LicenseFragment"
        android:label="@string/pref_license" />
</navigation>